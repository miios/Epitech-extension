{
  "manifest_version": 2,

  "name": "Epitech extension",
  "description": "A non official Epitech extension which allow you to see your last alert sent by epitech ! Made by Nathan Ann√©",
  "version" :"0.11",

  "browser_action": {
    "default_icon": "epitech-logo.png",
    "default_popup": "index.html",
    "default_title": "Check your notifications"
  },
  "icons": {
    "16": "epitech-logo.png",
    "48": "epitech-logo.png",
    "128": "epitech-logo.png"
  },

  "content_security_policy": "script-src 'self' 'sha256-JaJo+GWtiKB7YdhRMBY3713jCaKGVJi5HB7Uf9KXoKY='; object-src 'self'",
  "permissions": [
    "https://intra.epitech.eu/user/notification/message?format=json",
    "activeTab"
  ]
}